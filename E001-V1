class category:
    def __init__(self,name,code,no_of_product):
        self.name=name
        self.code=code
        self.no_of_product=no_of_product
        
ob1=category("aa",1011,11)
ob2=category("bb",1012,12)
ob3=category("cc",1013,13)
 
obj=[ob1,ob2,ob3]
for i in obj:
    print(i.no_of_product,i.name,i.code)
    
    
    